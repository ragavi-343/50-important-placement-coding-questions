#include <stdio.h>
int main() {
   int n;
   scanf("%d",&n);
   int a[n];
   for(int i=0;i<n;i++)
   {
       scanf("%d",&a[i]);
   }
   int s,f=0,v=0;
   scanf("%d",&s);
   for(int i=0;i<n;i++)
   {
       if(s==a[i])
       {
           f=1;
           v=i+1;
           break;
       }
   }
   if(f)
   {
        printf("element found at the index %d",v);
   }
   else
   {
       printf("element not found");
   }
    return 0;
}
