#include <stdio.h>
int main() {
    int a;
    scanf("%d",&a);
    int c=0;
    for(int i=2;i<a;i++)
    {
        if(a%i==0)
        {
            c=1;
            break;
        }
    }
    if(!c)
    {
        printf("%d is a prime number",a);
    }
    else
    {
        printf("%d is not a prime number",a);
    }
    return 0;
}